{
  "LambdaFunctionConfigurations": [
    {
      "Id": "TriggerUserProfileBuilderOnStage2Upload",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:YOUR_ACCOUNT_ID:function:user-profile-builder",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "suffix",
              "Value": ".json"
            },
            {
              "Name": "prefix",
              "Value": "stage2/"
            }
          ]
        }
      }
    }
  ]
}