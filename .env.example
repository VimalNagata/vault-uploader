# Google Authentication
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_S3_BUCKET_NAME=your-s3-bucket-name

# API Endpoints
REACT_APP_API_BASE_URL=https://12345abcde.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_CREDENTIALS_API_URL=https://12345abcde.execute-api.us-east-1.amazonaws.com/prod/credentials
REACT_APP_CATEGORIZE_API_URL=https://12345abcde.execute-api.us-east-1.amazonaws.com/prod/categorize
REACT_APP_USER_PROFILE_API_URL=https://12345abcde.execute-api.us-east-1.amazonaws.com/prod/user-profile
REACT_APP_METRICS_API_URL=https://12345abcde.execute-api.us-east-1.amazonaws.com/prod/user-data-metrics
REACT_APP_PROMPT_MANAGER_API_URL=https://12345abcde.execute-api.us-east-1.amazonaws.com/prod/prompt-manager

# Option 2: Direct Credentials (Development/Testing only - NOT RECOMMENDED FOR PRODUCTION)
# REACT_APP_AWS_ACCESS_KEY_ID=your-access-key-id
# REACT_APP_AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Set NODE_ENV to "development" for local development
NODE_ENV=development

# This is only needed on the Lambda side, not in the React app
# S3_ROLE_ARN=arn:aws:iam::[ACCOUNT_ID]:role/s3-user-access-role